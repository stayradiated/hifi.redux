{
  "name": "@stayradiated/hifi-redux",
  "version": "1.0.0",
  "description": "Redux store for hifi.kitchen",
  "main": "dist/index.js",
  "author": "George Czabania <george@czabania.com>",
  "license": "ISC",
  "scripts": {
    "build": "pkg-build",
    "prepublish": "pkg-build",
    "test": "pkg-test",
    "coverage": "pkg-coverage",
    "lint": "pkg-lint"
  },
  "devDependencies": {
    "@mishguru/package": "^2.1.4"
  },
  "dependencies": {
    "perplexed": "^2.0.0",
    "redux": "^3.7.2",
    "reselect": "^3.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 0.12
          },
          "modules": "commonjs",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
